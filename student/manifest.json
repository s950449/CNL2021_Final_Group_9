{
    "name": "Attendance checker (Student)",
    "description": "Online meeting attendance checker!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "storage",
        "scripting",
        "background"
    ],
    "action": {
        "default_popup": "/html/popup.html",
        "default_icon": {
            "16": "/images/app_icon16.png",
            "32": "/images/app_icon32.png",
            "48": "/images/app_icon48.png",
            "128": "/images/app_icon128.png"
        }
    },
    "icons": {
        "16": "/images/app_icon16.png",
        "32": "/images/app_icon32.png",
        "48": "/images/app_icon48.png",
        "128": "/images/app_icon128.png"
    }
}